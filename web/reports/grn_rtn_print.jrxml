	<queryString>
		<![CDATA[SELECT
     grn_return.`rtn_no` AS rtn_no,
     grn_return.`grn_no` AS grn_no,
     grn_return.`date` AS date,
     grn_return.`username` AS username,
     grn_return.`remarks` AS remarks,
     grn_return.`total` AStotal,
     grn_return_line.`item_id` ASitem_id,
     raw_item.`description` AS description,
     grn_return_line.`qty` AS line_qty,
     grn_return_line.`price` AS price,
     supplier.`name` AS name,
     supplier.`address1` AS address1,
     supplier.`address2` AS address2,
     supplier.`city` AS city,
     supplier.`postal_code` AS postal_code
FROM
     `grn_return` grn_return INNER JOIN `grn_return_line` grn_return_line ON grn_return.`rtn_no` = grn_return_line.`rtn_no`
     INNER JOIN `grn` grn ON grn_return.`grn_no` = grn.`grn_no`
     INNER JOIN `supplier` supplier ON grn.`sup_id` = supplier.`sup_id`
     INNER JOIN `raw_item` raw_item ON grn_return_line.`item_id` = raw_item.`item_id`
where grn_return.`rtn_no`=$P{id}]]>
	</queryString>
